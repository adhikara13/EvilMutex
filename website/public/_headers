/*
  X-Content-Type-Options: nosniff
  Referrer-Policy: strict-origin-when-cross-origin
  X-Robots-Tag: index, follow
  Cache-Control: public, max-age=31536000, immutable
  X-Frame-Options: DENY
  X-XSS-Protection: 1; mode=block

/
  Link: <https://evilmutex.com/>; rel="canonical"

/malware/*
  X-Robots-Tag: index, follow
  Cache-Control: public, max-age=3600
  Link: <https://evilmutex.com/:splat>; rel="canonical"

/contributor
  X-Robots-Tag: index, follow
  Cache-Control: public, max-age=3600
  Link: <https://evilmutex.com/contributor>; rel="canonical"

/_nuxt/*
  Cache-Control: public, max-age=31536000, immutable

/data/*
  Cache-Control: public, max-age=86400

# Cache fonts for a long time
/fonts/*
  Cache-Control: public, max-age=31536000, immutable

# Cache images for a long time
/images/*
  Cache-Control: public, max-age=31536000, immutable

# Don't cache HTML files too aggressively
/*.html
  Cache-Control: public, max-age=3600

# Serve pre-compressed Brotli assets if the browser supports it
/*.js.br
  Content-Type: application/javascript
  Content-Encoding: br

/*.css.br
  Content-Type: text/css
  Content-Encoding: br

# Serve pre-compressed Gzip assets if the browser supports it
/*.js.gz
  Content-Type: application/javascript
  Content-Encoding: gzip

/*.css.gz
  Content-Type: text/css
  Content-Encoding: gzip 